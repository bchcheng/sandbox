{
  "plugins": [],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
      # "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    # "react/jsx-no-undef": 2,
    # "react/jsx-uses-react": 2,
    # "react/jsx-uses-vars": 2,
    "no-unused-vars": 1,
    "prefer-const": 1,
    "semi": 1,
  }
}
